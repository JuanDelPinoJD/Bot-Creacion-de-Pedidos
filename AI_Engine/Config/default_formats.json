{
  "order_number_format": [
    "[a-zA-Z]*\\d+"
  ],
  "reference_format": [
    "[a-zA-Z]+\\d+"
  ],
  "number_format": [
    "[\\d\\.]+(,\\d*){0,1}",
    "[\\d,]+(\\.\\d*){0,1}"
  ],
  "date_format": [
    "\\d{1,2}(\\.|\\-|\/| )\\d{1,2}(\\.|\\-|/| )\\d{4}",
    "\\d{4}(\\.|\\-|\/| )\\d{1,2}(\\.|\\-|/| )\\d{2}",
    "\\d{2}(\\.|\\-|\/| )\\d{1,2}(\\.|\\-|/| )\\d{2}",
    "^[a-zA-Z]{3}[ \\-]\\d{2,4}$"
  ]
}