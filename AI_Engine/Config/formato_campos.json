{
  "order_number": [
    "^[a-zA-Z]*\\d+$"
  ],
  "reference": [
    "^[a-zA-Z]+\\d+$"
  ],
  "quantity": [
    "^[\\d\\.]+(,\\d*){0,1}$",
    "^[\\d,]+(\\.\\d*){0,1}$"
  ],
  "ship_out_date": [
    "^(\\d{1,2}\/)?\\d{1,2}\/\\d{2,4}$",
    "^(\\d{1,2}\\.)?\\d{1,2}\\.\\d{2,4}$",
    "^(\\d{1,2}\\-)?\\d{1,2}\\-\\d{2,4}$",
    "^\\d{2,4}\/\\d{1,2}\/\\d{1,2}$",
    "^\\d{2,4}\\.\\d{1,2}\\.\\d{1,2}$",
    "^\\d{2,4}\\-\\d{1,2}\\-\\d{1,2}$",
    "^[a-zA-Z]{3}[ \\-]\\d{2,4}$"
  ],
  "arrival_date": [
    "^(\\d{1,2}\/)?\\d{1,2}\/\\d{2,4}$",
    "^(\\d{1,2}\\.)?\\d{1,2}\\.\\d{2,4}$",
    "^(\\d{1,2}\\-)?\\d{1,2}\\-\\d{2,4}$",
    "^\\d{2,4}\/\\d{1,2}\/\\d{1,2}$",
    "^\\d{2,4}\\.\\d{1,2}\\.\\d{1,2}$",
    "^\\d{2,4}\\-\\d{1,2}\\-\\d{1,2}$",
    "^[a-zA-Z]{3}[ \\-]\\d{2,4}$"
  ]
}